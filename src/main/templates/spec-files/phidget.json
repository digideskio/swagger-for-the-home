{
  "apiVersion":"0.2",
  "swaggerVersion":"1.1",
  "basePath":"http://localhost:8002",
  "resourcePath":"/phidget",
  "apis":[
    {
      "path":"/phidget.{format}/lcd",
      "description":"Manipulates the LCD",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Updates the LCD",
          "notes":"",
          "responseClass":"void",
          "nickname":"setLcd",
          "parameters":[
            {
              "name":"lineNumber",
              "description":"Line to update",
              "paramType":"query",
              "required":true,
              "allowMultiple":false,
              "dataType":"int",
              "defaultValue":"0",
              "allowableValues":{
                "valueType":"LIST",
                "values":[
                  "0",
                  "1",
                  "2",
                  "3"
                ]
              }
            },
            {
              "name":"msg",
              "description":"text to send",
              "paramType":"query",
              "required":true,
              "allowMultiple":false,
              "dataType":"string"
            }
          ]
        }
      ]
    },
    {
      "path":"/phidget.{format}/lcd/contrast",
      "description":"Manipulates the LCD",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Updates the LCD",
          "notes":"",
          "responseClass":"void",
          "nickname":"setContrast",
          "parameters":[
            {
              "name":"value",
              "description":"contrast to set to",
              "paramType":"query",
              "required":true,
              "allowMultiple":false,
              "dataType":"int",
              "defaultValue":"200",
              "allowableValues":{
                "valueType":"RANGE",
                "min":"0",
                "max":"255"
              }
            }
          ]
        }
      ]
    },
    {
      "path":"/phidget.{format}/lcd/backlight",
      "description":"Manipulates the LCD backlight",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Updates the LCD backlight",
          "notes":"",
          "responseClass":"void",
          "nickname":"setBacklight",
          "parameters":[
            {
              "name":"enabled",
              "description":"turn on or off light",
              "paramType":"query",
              "required":true,
              "allowMultiple":false,
              "dataType":"boolean",
              "defaultValue":"true",
              "allowableValues":{
                "valueType":"LIST",
                "values":[
                  "true",
                  "false"
                ]
              }
            }
          ]
        }
      ]
    }
  ],
  "models":{}
}